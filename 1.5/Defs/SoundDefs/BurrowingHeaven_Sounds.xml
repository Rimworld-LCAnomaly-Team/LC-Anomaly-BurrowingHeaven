<?xml version="1.0" encoding="utf-8"?>
<Defs>

  <SoundDef>
    <defName>BurrowingHeaven_loop</defName>
    <sustain>true</sustain>
    <context>MapOnly</context>
    <maxSimultaneous>10</maxSimultaneous>
    <priorityMode>PrioritizeNearest</priorityMode>
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>Pawn/BurrowingHeaven/MustSee_Tree</clipPath>
          </li>
        </grains>
        <volumeRange>30</volumeRange>
        <distRange>30~100</distRange>
        <sustainRelease>5</sustainRelease>
      </li>
    </subSounds>
  </SoundDef>

  <SoundDef>
    <defName>BurrowingHeaven_attack</defName>
    <context>MapOnly</context>
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Folder">
            <clipFolderPath>Pawn/BurrowingHeaven/Attack</clipFolderPath>
          </li>
        </grains>
        <volumeRange>30</volumeRange>
        <pitchRange>0.95~1.05</pitchRange>
        <sustainLoop>false</sustainLoop>
      </li>
    </subSounds>
  </SoundDef>

  <SoundDef>
    <defName>BurrowingHeaven_death
    </defName>
    <context>MapOnly</context>
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>Pawn/BurrowingHeaven/MustSee_Dead</clipPath>
          </li>
        </grains>
        <volumeRange>30</volumeRange>
        <pitchRange>0.95~1.05</pitchRange>
        <sustainLoop>false</sustainLoop>
      </li>
    </subSounds>
  </SoundDef>

</Defs>